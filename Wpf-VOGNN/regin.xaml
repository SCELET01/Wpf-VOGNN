<Window x:Class="Wpf_VOGNN.regin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf_VOGNN"
        mc:Ignorable="d"
        Title="regin" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="TextBox_login" Margin="300,131,300,285" TextWrapping="Wrap"/>
        <Button x:Name="reg" Click="reg_Click" Content="Регистрация" Margin="300,250,300,164"/>
        <Button x:Name="back" Click="back_Click" Content="Отмена" Margin="300,275,300,139"/>
        <PasswordBox x:Name="password" Margin="300,174,300,242"/>
        <Label Content="Логин:" Margin="300,105,454,303"/>
        <Label Content="Пароль:" Margin="300,149,446,260"/>
        <Label Content="Повторите пароль:" Margin="300,192,382,217" Width="NaN"/>
        <PasswordBox x:Name="password_Copy" Margin="300,217,300,199"/>

    </Grid>
</Window>
