<Page x:Class="Wpf_VOGNN.Pages.Spisok"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Wpf_VOGNN.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Spisok">

    <Grid>
        <Border Background="#66FFFFFF" CornerRadius="20">
            <DataGrid x:Name="DataGridSpisok" d:ItemsSource="{d:SampleData ItemCount=5}" Height="370" VerticalAlignment="Top" HorizontalAlignment="Center" Width="720" Margin="0,40,0,0" SelectionChanged="DataGridSpisok_SelectionChanged"/>
                
            
        </Border>
    </Grid>
</Page>
